<script type="text/javascript">
	function openWin(){
		let url="js16_window02.html";
		let target="_blank";
		let prop="width=500px,height=500px";
		window.open(url,target,prop);
	}
	
	function registForm(){
		let url="js17_window03.html";
		let target="_blank";
		let prop="width=600px,height=600px";
		window.open(url,target,prop);
	}
</script>
